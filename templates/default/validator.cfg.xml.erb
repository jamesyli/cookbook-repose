<?xml version="1.1" encoding="UTF-8"?>

<!-- https://repose.atlassian.net/wiki/display/REPOSE/API+Validation+filter -->
<validators multi-role-match="true" xmlns='http://openrepose.org/repose/validator/v1.0'>
   <validator
      role="default"
      default="true"
      wadl="<%= @wadl %>"
